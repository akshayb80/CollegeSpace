{
    "name": "collegespace",
    "version": "0.1.0",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "react-client": "cd react-client && npm start",
        "server": "cd backend && nodemon server.js",
